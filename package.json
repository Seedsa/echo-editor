{
  "name": "echo-editor",
  "version": "0.4.7",
  "private": false,
  "license": "MIT",
  "description": "A modern AI-powered WYSIWYG rich-text editor for Vue, based on Tiptap and shadcn-vue",
  "author": "SEED<hello.mr.lji@gmail.com>",
  "homepage": "https://github.com/Seedsa/echo-editor/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Seedsa/echo-editor/"
  },
  "scripts": {
    "dev": "vite",
    "examples": "pnpm build:lib && pnpm --dir ./examples run dev",
    "build:lib": "vite build",
    "build:examples": "pnpm build:lib && pnpm --dir ./examples run build",
    "preview": "vite preview",
    "git-commit": "git-cz",
    "type:check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx,.vue src --fix",
    "push": "pnpm build:lib && pnpm publish --no-git-checks --registry https://registry.npmjs.org/",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "keywords": [
    "echo editor",
    "WYSIWYG",
    "vue",
    "vue.js",
    "typescript",
    "tiptap",
    "editor",
    "shadcn ui",
    "wysiwyg",
    "rich text",
    "markdown"
  ],
  "main": "./lib/echo-editor.umd.js",
  "module": "./lib/echo-editor.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/echo-editor.mjs",
      "require": "./lib/echo-editor.umd.js"
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "sideEffects": false,
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.0",
    "@tiptap/core": "next",
    "@tiptap/extension-blockquote": "next",
    "@tiptap/extension-bold": "next",
    "@tiptap/extension-bubble-menu": "next",
    "@tiptap/extension-bullet-list": "next",
    "@tiptap/extension-character-count": "next",
    "@tiptap/extension-code": "next",
    "@tiptap/extension-color": "next",
    "@tiptap/extension-document": "next",
    "@tiptap/extension-dropcursor": "next",
    "@tiptap/extension-focus": "next",
    "@tiptap/extension-font-family": "next",
    "@tiptap/extension-gapcursor": "next",
    "@tiptap/extension-hard-break": "next",
    "@tiptap/extension-heading": "next",
    "@tiptap/extension-highlight": "next",
    "@tiptap/extension-history": "next",
    "@tiptap/extension-horizontal-rule": "next",
    "@tiptap/extension-image": "next",
    "@tiptap/extension-italic": "next",
    "@tiptap/extension-link": "next",
    "@tiptap/extension-list": "3.0.0-next.4",
    "@tiptap/extension-list-item": "next",
    "@tiptap/extension-ordered-list": "next",
    "@tiptap/extension-paragraph": "next",
    "@tiptap/extension-placeholder": "next",
    "@tiptap/extension-strike": "next",
    "@tiptap/extension-subscript": "next",
    "@tiptap/extension-superscript": "next",
    "@tiptap/extension-table": "next",
    "@tiptap/extension-table-cell": "next",
    "@tiptap/extension-table-header": "next",
    "@tiptap/extension-table-row": "next",
    "@tiptap/extension-task-item": "next",
    "@tiptap/extension-task-list": "next",
    "@tiptap/extension-text": "next",
    "@tiptap/extension-text-align": "next",
    "@tiptap/extension-text-style": "next",
    "@tiptap/extension-underline": "next",
    "@tiptap/extensions": "3.0.0-next.4",
    "@tiptap/pm": "next",
    "@tiptap/suggestion": "next",
    "@tiptap/vue-3": "next",
    "@vueuse/core": "^10.11.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hotkeys-js": "^3.13.7",
    "lucide-vue-next": "^0.477.0",
    "reka-ui": "^2.0.2",
    "tippy.js": "^6.3.7",
    "y-prosemirror": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@iconify/vue": "^4.1.2",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "express": "^4.19.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "lodash-unified": "^1.0.3",
    "prettier": "^3.2.5",
    "prism-code-editor": "^3.4.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "1.75.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  },
  "peerDependencies": {
    "vue": ">= 3.0.0"
  }
}
